IF
  Detect([PC])
  NumberOfTimesTalkedTo(0)
  Global("Iapcastts","GLOBAL",0)
THEN
  RESPONSE #100
    StartDialogNoSet([PC])
END

IF
  InMyArea(Player1)
  OR(2)
   NumberOfTimesTalkedTo(0)
   Global("Iadraggr01","LOCALS",0)
  Global("Iareaction","ARia17",0)
  OR(5)
   AttackedBy([GOODCUTOFF],DEFAULT)
   AttackedBy([PC],DEFAULT)
   SpellCastOnMe([GOODCUTOFF],0)
   SpellCastOnMe([PC],0)
   TookDamage()
THEN
  RESPONSE #100
    SetGlobal("Iareaction","ARia17",1)
END

IF
  Allegiance(Myself,ENEMY)
  InMyArea(Player1)
  OR(2)
   See(NearestEnemyOf(Myself))
   Global("Iareaction","ARia17",1)
  Global("Iadraggr01","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Iadraggr01","LOCALS",1)
    SetGlobal("Iaattack","LOCALS",2)
    ReallyForceSpellRES("SPWII03",Myself) //
    ReallyForceSpellRES("SPPRI05",Myself) //
    ReallyForceSpell(Myself,DRAGON_STONE_SKIN)
    ReallyForceSpellRES("SPWII01",Myself) //
END

IF
  See([PC])
  OR(3)
   Alignment(Player1,MASK_EVIL)
   GlobalGT("Iap1notgood","GLOBAL",0)
   !AreaCheck("ARia17")
THEN
  RESPONSE #100
    Kill(Player1)
    Kill(LastSeenBy(Myself))
END

IF
  Allegiance(Myself,ENEMY)
  !HPLT(Myself,1)
  See(Player1)
  Class(Player1,MAGE_ALL)
  !CheckStat(Player1,5,SCRIPTINGSTATE7)
  !CheckStatGT(Player1,0,SCRIPTINGSTATE2)
  !StateCheck(Player1,STATE_MIRRORIMAGE)
  CheckStatLT(Player1,51,RESISTMAGIC)
  !CheckStat(Player1,2,SCRIPTINGSTATE3)
  CheckStatGT(Player1,7,SAVEVSDEATH)
  !HasBounceEffects(Player1)
  Global("Iapcastts","GLOBAL",0)
  Global("Iap1nr","GLOBAL",0)
  Global("Iadraggr08","LOCALS",0)
