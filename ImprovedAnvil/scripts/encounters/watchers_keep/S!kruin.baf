END

IF
  Allegiance(Myself,ENEMY)
  Global("Iacreature","LOCALS",0)
  See([PC])
THEN
  RESPONSE #100
    SetGlobal("Iacreature","LOCALS",1)
    CreateCreatureObjectDoor("S!elegol",LastSeenBy(Myself),0,0,0) //
    CreateCreatureObjectDoor("S!elegol",LastSeenBy(Myself),0,0,0) //
END

IF
  Allegiance(Myself,ENEMY)
  OR(2)
   Detect([PC])
   Detect([GOODCUTOFF])
