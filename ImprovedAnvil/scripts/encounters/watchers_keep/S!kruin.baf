// Fighter Mage Levels 30 25 //
// First level spells (5): MM x5 //
// Second level spells (5): Acid arrow x5 //
// Third level spells (5): Remove Magic x2, Flame Arrow x3 //
// Fourth level spells (5): Stoneskin, Imp invis, Fire Shield, Minor Globe //
// Fifth level spells (5): Spell Immunity, Breach x4 //
// Sixth level spells (6): True Sight x3, Death Spell x3, nishruu //
// Seventh level spells (6): Spell Turning, FoD, Ruby Ray x4//
// Eighth level spells (6): ADHW x6 //
// Ninth level spells (5): Power word kill, comet x4//
// Scrolls: pfmw x5, ruby ray x5 //

IF
  Global("KruinTalked","GLOBAL",0)
  InMyArea(Player1)
  !See([PC])
THEN
  RESPONSE #100
    MoveToObject(Player1)
END

IF
  Global("KruinTalked","GLOBAL",0)
  Global("Iapcastts","GLOBAL",0)
  See([PC])
THEN
  RESPONSE #100
    StartDialogNoSet([PC])
END

IF
  Allegiance(Myself,ENEMY)
  OR(2)
   See(NearestEnemyOf(Myself))
   Detect([PC])
  Global("Iakruin02","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Iakruin02","LOCALS",1)
    ReallyForceSpellRES("SPWII41",Myself)
    ReallyForceSpell(Myself,WARRIOR_HARDINESS)
    ReallyForceSpell(Myself,WIZARD_SPELL_IMMUNITY_DIVINATION)
    ReallyForceSpell(Myself,WIZARD_STONE_SKIN)
    ReallyForceSpell(Myself,WIZARD_FIRE_SHIELD_BLUE)
    ReallyForceSpell(Myself,WIZARD_MINOR_GLOBE_OF_INVULNERABILITY)
END

IF
  Detect([PC])
  Global("Iatreasure","LOCALS",0)
  Global("Iawarbladeplace","GLOBAL",1)
THEN
  RESPONSE #100
    SetGlobal("Iawarbladeplace","GLOBAL",5)
    SetGlobal("Iatreasure","LOCALS",1)
    GiveItemCreate("sw2h09",Myself,0,0,0)  // Warblade +4
END

IF
  Allegiance(Myself,ENEMY)
  Global("Iapcastts","GLOBAL",0)
  HasItem("minhp1",Myself)
THEN
  RESPONSE #100
    SetInterrupt(FALSE)
    ReallyForceSpellRES("SPWII07",Myself)
    DestroyItem("minhp1")
    SetInterrupt(TRUE)
END

IF
  !HasItem("minhp1",Myself)
  Global("Iapcastts","GLOBAL",1)
  Global("Iatsinvulner","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Iatsinvulner","LOCALS",1)
    ApplySpellRES("SPINI70",Myself)
END

IF
  Global("Iapcastts","GLOBAL",0)
