// Time stop //

IF
  Allegiance(Myself,ENEMY)
  !GlobalTimerNotExpired("Iaspell","LOCALS")
  OR(2)
   Global("Iapcastts","GLOBAL",1)
   CheckStatGT(Myself,0,SCRIPTINGSTATE9)
THEN
  RESPONSE #100
    SetGlobalTimer("Iaspell","LOCALS",6)
END

// SI:Abj //

IF
  Allegiance(Myself,ENEMY)
  See(NearestEnemyOf(Myself))
  GlobalGT("Prep","LOCALS",0)
  !GlobalTimerNotExpired("Iaspell","LOCALS")
  OR(2)
    !GlobalTimerNotExpired("Iasiduration","LOCALS")
    CheckStat(Myself,0,SCRIPTINGSTATE7)
  OR(3)
   CheckStatGT(Myself,0,SCRIPTINGSTATE6)
   CheckStatGT(Myself,0,STONESKINS)
   !Range(LastSeenBy(Myself),4)
  GlobalLT("IaDeathKnight11","LOCALS",3)
THEN
  RESPONSE #100
    IncrementGlobal("IaDeathKnight11","LOCALS",1)
    SetGlobalTimer("Iaspell","LOCALS",6)
    SetGlobalTimer("Iasiduration","LOCALS",123)
    ForceSpell(Myself,WIZARD_SPELL_IMMUNITY_ABJURATION)
END

