IF
  Detect(NearestEnemyOf(Myself))
  Global("Iacutscene","LOCALS",0)
THEN
  RESPONSE #100
    SetGlobal("Iacutscene","LOCALS",1)
    ClearAllActions()
    StartCutSceneMode()
    StartCutScene("S!cutor")
END

